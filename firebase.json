{
  "hosting": [
    {
      "site": "veganmapai-1a8b7",
      "public": "dist/public",
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
      ],
      "rewrites": [
        {
          "source": "**",
          "destination": "/index.html"
        }
      ],
      "headers": [
        {
          "source": "**",
          "headers": [
            { "key": "Cache-Control", "value": "no-cache, no-store, must-revalidate" },
            { "key": "Pragma", "value": "no-cache" },
            { "key": "Expires", "value": "Thu, 01 Jan 1970 00:00:00 GMT" }
          ]
        }
      ]
    }
  ]
}